<html>
  <head>
    <script type="text/javascript">
      function redirectToAuth() {
        if (window.top) {
          window.top.location = '{{auth_url}}';
        }
        else {
          window.location = '{{auth_url}}';
        }
      }
    </script>
  </head>

  <body onload="redirectToAuth()">
  </body>
</html>
